<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="get_crm_cuota_report_form" model="ir.ui.view"> 
    <field name="name">get.crm.cuota.report.form</field> 
    <field name="model">get.crm.cuota.report</field>
    <field name="arch" type="xml">
      <form>
          <sheet>
            <group>
                <field name="team_ids" widget="many2many_tags"/>
            </group>
          </sheet>
          <footer>
            <button name="create_report" id="confirm" string="Generar reporte" type="object" class="btn-primary"/>
            <button string="Cancelar" class="btn-secondary" special="cancel"/>
          </footer>
      </form>
    </field> 
  </record>
</odoo>