<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="crm_cuota_report_tree" model="ir.ui.view">
    <field name="name">crm.cuota.report.tree</field> 
    <field name="model">crm.cuota.report</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="user_id" widget="many2one_avatar_user"/>
        <field name="team_id"/>
        <field name="fecha_inicio"/>
        <field name="fecha_fin"/>
        <field name="cuota"/>
        <field name="cuota_cubierta"/>
        <field name="cuota_restante"/>
      </tree>
    </field> 
  </record>

</odoo>
